{
  "name": "@kodebase/artifacts",
  "version": "1.1.1",
  "description": "High-level artifact operations for Kodebase",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prebuild": "pnpm clean",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "lint": "biome lint .",
    "lint:package": "publint",
    "lint:types": "attw --pack . --profile esm-only",
    "format": "biome format .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "docs": "typedoc",
    "docs:coverage": "typedoc --plugin typedoc-plugin-coverage",
    "prepack": "pnpm lint:package"
  },
  "dependencies": {
    "@kodebase/core": "workspace:*",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@kodebase/typescript-config": "workspace:*",
    "@types/node": "^22.19.0",
    "@vitest/coverage-istanbul": "^3.0.7",
    "memfs": "^4.50.0",
    "publint": "^0.3.15",
    "typedoc": "^0.28.14",
    "typedoc-plugin-coverage": "^4.0.2"
  }
}
